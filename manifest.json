{
    "manifest_version": 2,
    "name": "Automatic Library Access for SIM",
    "version": "1.0.0",
    "version_name": "1.0.0 pilot run",
    "author": "Phone Thant Ko",
    "description": "Automatically load library resources using SIM LibProxy by Phone Thant Ko <phonethantko@outlook.com>",
    "background": {
      "scripts": ["js/lso.js", "js/parseuri.js", "js/resources.js", "js/background.js"]
    },
    "options_page": "html/options.html",
    "permissions": [
      "tabs", "webNavigation", "notifications"
    ],
    "browser_action": {
      "default_title": "Reload using SIMLib Proxy"
    }
}
